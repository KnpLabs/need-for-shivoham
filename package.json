{
  "name": "reactive",
  "version": "0.1.0",
  "private": true,
  "devDependencies": {
    "browser-sync": "^2.18.12",
    "concurrently": "^3.5.0",
    "cycle-scripts": "1.1.0",
    "nodemon": "^1.11.0",
    "webpack": "^3.0.0"
  },
  "dependencies": {
    "@cycle/dom": "^17.4.0",
    "@cycle/run": "^3.1.0",
    "@cycle/time": "^0.8.0",
    "xstream": "^10.8.0"
  },
  "scripts": {
    "webpack": "webpack --colors --progress",
    "webpack:watch": "nodemon -w src --exec 'npm run webpack'",
    "browser-sync": "browser-sync start -f public/bundle.js -s public",
    "start": "concurrently 'npm run webpack:watch' 'npm run browser-sync'",
    "test": "cycle-scripts test",
    "build": "cycle-scripts build",
    "eject": "cycle-scripts eject"
  },
  "babel": {
    "presets": [
      "es2015"
    ]
  }
}
